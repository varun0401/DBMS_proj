<body>
    <div class="container">
        <div class="left-column">
            <!-- User info section -->
            <h2>User Info</h2>
            
            <div>
              <% presenting_notes.forEach(note => { %>
                <p><%= note.notes %></p>
              <% }) %>
            </div>
            <div class="user-info">
                <div class="user_id"><p id="user_id">User ID: <%= userId %></p>  </div>              
               <div class="name"> <p id="name">Name:<%= name %></p> </div>
      
              </div>
        </div>
    </div>
